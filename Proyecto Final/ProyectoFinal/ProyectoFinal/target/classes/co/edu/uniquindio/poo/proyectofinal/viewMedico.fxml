<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.proyectofinal.viewController.MedicoViewController">
   <children>
      <Button id="btnHistorialMedico" fx:id="btnHistorialMedico" layoutX="210.0" layoutY="21.0" mnemonicParsing="false" onAction="#cambiarVentana" text="Historial Medico" />
      <Button id="btnDiagnostico" fx:id="btnDiagnostico" layoutX="325.0" layoutY="21.0" mnemonicParsing="false" onAction="#cambiarVentana" text="Diagnostico" />
      <Button id="btnHorarios" fx:id="btnHorario" layoutX="507.0" layoutY="21.0" mnemonicParsing="false" onAction="#cambiarVentana" text="Horarios" />
